{
  "name": "artcritical-list",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "webpack-dev-server --inline --content-base src/ --history-api-fallback",
    "start:prod": "npm run build && node src/server.bundle.js",
    "build:client": "webpack",
    "build:server": "webpack --config webpack.server.config.js",
    "build": "npm run build:server && npm run build:client"
  },
  "dependencies": {
    "alt": "^0.18.6",
    "asap": "^2.0.6",
    "aws-sdk": "^2.92.0",
    "babel-core": "^6.13.2",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "ejs": "^2.5.7",
    "express": "^4.14.1",
    "if-env": "^1.0.0",
    "jade": "~1.11.0",
    "morgan": "~1.8.1",
    "node-jsx": "^0.13.3",
    "pug": "^2.0.0-rc.2",
    "react": "^15.6.1",
    "react-dom": "^15.3.2",
    "react-intl": "^2.3.0",
    "react-router": "^2.8.1",
    "serve-favicon": "~2.4.2"
  },
  "description": "A list tool for artcritical",
  "main": "app.js",
  "devDependencies": {
    "async": "^2.5.0",
    "babel-cli": "^6.24.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "http-server": "^0.9.0",
    "mongodb": "^2.2.30",
    "mongoimport": "^1.0.6",
    "mongoose": "^4.11.5",
    "webpack": "^3.0.0-rc.2"
  },
  "author": "13milliseconds",
  "license": "ISC"
}
